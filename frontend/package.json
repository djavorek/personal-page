{
  "name": "personal-page",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build --modern=client",
    "start": "nuxt start --modern=client",
    "generate": "nuxt generate --modern",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "npm run lint:js",
    "clean": "rm -rf ./node_modules",
    "clean:install": "npm run clean && npm i",
    "clean:build": "npm run clean:install && rm -rf /dist ./nuxt && npm run generate"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.17.2",
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/vue-fontawesome": "^2.0.6",
    "@nuxt/babel-preset-app": "^2.15.8",
    "@nuxt/content": "^1.15.1",
    "@nuxtjs/fontawesome": "^1.1.2",
    "@nuxtjs/sitemap": "^2.4.0",
    "core-js": "^3.21.1",
    "lodash": "^4.17.21",
    "markdown-it": "^12.3.2",
    "nuxt": "^2.15.8",
    "query-string": "^7.1.1",
    "sparkpost": "^2.1.4",
    "v-currency-field": "^3.1.1",
    "vue-disqus": "^4.0.1",
    "vue-lazyload": "^1.3.4",
    "vue-moment": "^4.1.0",
    "vue-smooth-reflow": "^0.1.12"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/vuetify": "^1.12.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-vue": "^7.20.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "^2.5.1",
    "sass": "^1.49.9",
    "sass-loader": "^10.2.1",
    "string.prototype.includes": "^2.0.0"
  }
}
